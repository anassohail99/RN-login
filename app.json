{
  "expo": {
    "name": "LoginScreen",
    "slug": "LoginScreen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "sdkVersion": "38.0.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.abs.LoginScreen"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}